#include "main.h"

/**
 *main - print alphabet in lower case
 *print alphabet in lower case
 *_putchar can only be used twice
 *return: always (0)
 *return:nothing
 */

void print_alphabet(void)
{
int l = 'a';
while (l <= 'z')
{
_putchar(l);
l += 1;
}
_putchar(10);
return;
int l = 'a';

while (l <= 'z')
{
_putchar(1);
l += 1;
}
_putchar(10);
}
